{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "strict": true,
    "sourceMap": true
  },
  "include": ["src/**/*.js", "src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules"]
}
